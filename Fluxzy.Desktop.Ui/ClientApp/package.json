{
    "name": "fluxzy",
    "version": "0.1.18",
    "description": "A desktop app for for analyzing, debugging, and altering HTTP/1.1, H2, WebSocket traffic",
    "author": "Haga Rakotoharivelo",
    "copyright": "Copyright (c) 2022 Haga Rakotoharivelo",
    "keywords": [],
    "main": "app/main.js",
    "private": true,
    "scripts": {
        "ng": "ng",
        "start:webstorm": "npm-run-all -p electron:serve ng:serve",
        "start": "electron-forge start",
        "ng:serve": "ng serve -c web",
        "build": "npm run electron:serve-tsc && ng build --base-href ./",
        "build:dev": "npm run build -- -c dev",
        "build:prod": "npm run build -- -c production",
        "web:build": "npm run build -- -c web-production",
        "electron": "electron",
        "electron:serve-tsc": "tsc -p tsconfig.serve.json",
        "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && electron . --serve",
        "electron:local": "npm run build:prod && electron .",
        "test": "ng test --watch=false",
        "test:watch": "ng test",
        "e2e": "npm run build:prod && playwright test -c e2e/playwright.config.ts e2e/",
        "e2e:show-trace": "playwright show-trace e2e/tracing/trace.zip",
        "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
        "lint": "ng lint",
        "fluxzyd:build": "dotnet publish --self-contained --runtime win-x64 -c Release -o .publish  ../Fluxzy.Desktop.Ui.csproj",
        "fluxzyd:build-linux": "dotnet publish --self-contained --runtime linux-x64 -c Release -o .publish  ../Fluxzy.Desktop.Ui.csproj",
        "package-win": "npm run fluxzyd:build && npm run build:prod && electron-forge package",
        "package-linux": "npm run fluxzyd:build-linux && npm run build:prod && electron-forge package",
        "make": "electron-forge make"
    },
    "dependencies": {
        "@angular-devkit/core": "14.2.7",
        "@angular/common": "^14.2.8",
        "@angular/compiler": "^14.2.8",
        "@angular/core": "^14.2.8",
        "@angular/forms": "^14.2.8",
        "@angular/language-service": "^14.2.8",
        "@angular/platform-browser": "^14.2.8",
        "@angular/platform-browser-dynamic": "^14.2.8",
        "@angular/router": "^14.2.8",
        "@fontsource/roboto": "4.5.7",
        "@ng-icons/ionicons": "22.3.0",
        "angular-font-awesome": "3.1.2",
        "angular-split": "13.2.0",
        "bootstrap": "^5.1.3",
        "bootstrap-icons": "1.9.1",
        "electron-squirrel-startup": "1.0.0",
        "font-awesome": "4.7.0",
        "ionicons": "6.0.3",
        "lodash": "4.17.21",
        "material-icons": "1.11.6",
        "ngx-bootstrap": "9.0.0",
        "ngx-perfect-scrollbar": "10.1.1",
        "random-seed": "0.3.0",
        "rxjs": "7.5.5",
        "tslib": "^2.1.0",
        "uuid": "8.3.2",
        "zone.js": "~0.11.5"
    },
    "devDependencies": {
        "@angular-builders/custom-webpack": "14.0.1",
        "@angular-devkit/build-angular": "14.2.7",
        "@angular-eslint/builder": "^14.1.2",
        "@angular-eslint/eslint-plugin": "^14.1.2",
        "@angular-eslint/eslint-plugin-template": "^14.1.2",
        "@angular-eslint/schematics": "^14.1.2",
        "@angular-eslint/template-parser": "^14.1.2",
        "@angular/cli": "14.2.7",
        "@angular/compiler-cli": "^14.2.8",
        "@electron-forge/cli": "6.0.4",
        "@electron-forge/maker-deb": "6.0.4",
        "@electron-forge/maker-rpm": "6.0.4",
        "@electron-forge/maker-squirrel": "6.0.4",
        "@electron-forge/maker-zip": "6.0.4",
        "@ngx-translate/core": "14.0.0",
        "@ngx-translate/http-loader": "7.0.0",
        "@playwright/test": "1.27.1",
        "@types/jasmine": "3.10.3",
        "@types/jasminewd2": "2.0.10",
        "@types/lodash": "4.14.186",
        "@types/node": "17.0.23",
        "@typescript-eslint/eslint-plugin": "5.17.0",
        "@typescript-eslint/parser": "5.17.0",
        "conventional-changelog-cli": "2.2.2",
        "electron": "23.1.3",
        "electron-debug": "3.2.0",
        "electron-reloader": "1.2.3",
        "eslint": "8.12.0",
        "eslint-plugin-import": "2.25.4",
        "eslint-plugin-jsdoc": "38.1.6",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "jasmine-core": "3.10.1",
        "jasmine-spec-reporter": "7.0.0",
        "karma": "6.3.17",
        "karma-coverage-istanbul-reporter": "3.0.3",
        "karma-electron": "7.3.0",
        "karma-jasmine": "4.0.2",
        "karma-jasmine-html-reporter": "1.7.0",
        "node-polyfill-webpack-plugin": "1.1.4",
        "npm-run-all": "4.1.5",
        "playwright": "1.27.1",
        "ts-node": "10.7.0",
        "typescript": "~4.8.4",
        "wait-on": "6.0.1",
        "webdriver-manager": "12.1.8"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "browserslist": [
        "chrome 100"
    ]
}
