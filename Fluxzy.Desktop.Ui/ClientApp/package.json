    "@microsoft/signalr": "6.0.8",
    "@ng-icons/ionicons": "22.3.0",
    "angular-split": "13.2.0",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "1.9.1",
    "ionicons": "6.0.3",
    "lodash": "4.17.21",
    "material-icons": "1.11.6",
    "ngx-bootstrap": "8.0.0",
    "ngx-perfect-scrollbar": "10.1.1",
    "random-seed": "0.3.0",
    "rxjs": "7.5.5",
    "tslib": "^2.1.0",
    "uuid": "8.3.2",
    "zone.js": "~0.11.5"
  },
